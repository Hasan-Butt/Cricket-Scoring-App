     <property name="styleSheet">
      <string notr="true">/* Base style for all QPushButton */
QPushButton {
    /* Gradient background */
    background-color: qlineargradient(
        x1:0, y1:0, x2:0, y2:1,
        stop:0 #6db3f2, stop:1 #1e69de
    );
    color: white;
    border: 1px solid #1e69de;
    border-radius: 8px;
    font-size: 14px;
}

/* Hover state */
QPushButton:hover {
    background-color: qlineargradient(
        x1:0, y1:0, x2:0, y2:1,
        stop:0 #7dc4f4, stop:1 #2a7ae0
    );
}

/* Pressed state */
QPushButton:pressed {
    background-color: qlineargradient(
        x1:0, y1:0, x2:0, y2:1,
        stop:0 #1e69de, stop:1 #6db3f2
    );
    padding-top: 7px; /* slight shift */
}

/* Disabled state */
QPushButton:disabled {
    background-color: #cccccc;
    color: #777777;
    border: 1px solid #999999;
}

/* Example for a flat/icon-only button with objectName &quot;homeButton&quot; */
QPushButton#homeButton {
    background: transparent;
    border: none;
    padding: 4px;
}

/* If you need icon size via style */
QPushButton {
    /* adjust icon size if needed */
    qproperty-iconSize: 24px;
}

/* Optional: different styles for primary vs secondary */
QPushButton.primary {
    background-color: #27ae60;
    border: 1px solid #1e8449;
}
QPushButton.primary:hover {
    background-color: #2ecc71;
}
QPushButton.secondary {
    background-color: #f39c12;
    border: 1px solid #d68910;
}
QPushButton.secondary:hover {
    background-color: #f1c40f;
}
</string>
