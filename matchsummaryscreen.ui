    background: #0078D7;
    color: #FFFFFF;
    padding: 6px;
    border: 1px solid #A0A8B0;
    font-weight: bold;
}

/* Vertical headers (if used) */
QHeaderView::vertical-header {
    background: #F0F4F8;
    border: 1px solid #A0A8B0;
}

/* Table cells */
