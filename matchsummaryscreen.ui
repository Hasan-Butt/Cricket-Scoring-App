/* Remove default base under the tabs */
QTabBar {
    qproperty-drawBase: 0;
}

/* Unselected tabs */
QTabBar::tab {
    background: #FFFFFF;
    color: #555F6A;
    padding: 8px 20px;
    margin-right: 2px;
    border: 1px solid #A0A8B0;
    border-bottom: none;
