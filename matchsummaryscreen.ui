    border-top-right-radius: 6px;
    min-width: 100px;
}

/* Hover state */
QTabBar::tab:hover {
    background: #E4E9EF;
}

/* Selected tab */
QTabBar::tab:selected {
    background: #FFFFFF;
    color: #0078D7;
	 font-size:20px;
    border-color: #0078D7;
    border-bottom: 1px solid #F0F4F8; /* blend into pane */
    font-weight: bold;
}

/* Push down unselected tabs slightly */
QTabBar::tab:!selected {
    margin-top: 4px;
}

