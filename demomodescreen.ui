}

/* Remove default base under the tabs */
QTabBar {
    qproperty-drawBase: 0;
}

/* Unselected tabs */
QTabBar::tab {
    background: #FFFFFF;
    color: #555F6A;
    padding: 8px 20px;
    margin-right: 2px;
    border: 1px solid #A0A8B0;
    border-bottom: none;
    border-top-left-radius: 6px;
    border-top-right-radius: 6px;
    min-width: 100px;
}

/* Hover state */
QTabBar::tab:hover {
    background: #E4E9EF;
}

/* Selected tab */
QTabBar::tab:selected {
    background: #FFFFFF;
    color: #0078D7;
	 font-size:20px;
    border-color: #0078D7;
    border-bottom: 1px solid #F0F4F8; /* blend into pane */
    font-weight: bold;
}

/* Push down unselected tabs slightly */
QTabBar::tab:!selected {
    margin-top: 4px;
}

